{
  "name": "site",
  "version": "1.0.0",
  "description": "Basic website for ff",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node server.js",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/tape/bin/tape ./test/_runner.js && istanbul check-coverage --statements 60 --functions 60 --lines 60 --branches 50",
    "coverage": "istanbul cover ./node_modules/.bin/tape ./test/*.js -r html -o ./test/coverage.html"
  },
