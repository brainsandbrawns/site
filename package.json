  "bugs": {
    "url": "https://github.com/brainsandbrawns/site/issues"
  },
  "homepage": "https://github.com/brainsandbrawns/site#readme",
  "dependencies": {
    "handlebars": "^4.0.6",
    "hapi": "^15.2.0",
    "hoek": "^4.1.0",
    "inert": "^4.0.2",
    "vision": "^4.1.0"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "tape": "^4.6.3"
  }
}
